<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <FrameLayout
        android:id="@+id/FrameLayout1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" >

        <com.who.onecupafterwork.view.ImageViewRounded
            android:id="@+id/profileImageView"
            android:layout_width="147dp"
            android:layout_height="147dp"
            android:layout_gravity="center"
            android:layout_marginBottom="15dp"
            android:layout_marginLeft="106.6dp"
            android:layout_marginRight="106.6dp"
            android:layout_marginTop="15dp"
            android:background="@drawable/joinpicture_sel"
            android:src="@drawable/join_picture_1" />

        <ImageView
            android:id="@+id/imageViewAddPhoto"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="80dp"
            android:layout_marginTop="60dp"
            android:src="@drawable/join_toast_picture" />

    </FrameLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp" >

            <FrameLayout
                android:layout_width="150dp"
                android:layout_height="37dp"
                android:layout_marginRight="20dp"
                android:background="@drawable/join_name_1" >

                <EditText
                    android:id="@+id/nameView"
                    android:layout_width="104dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="right"
                    android:layout_marginRight="4dp"
                    android:background="@null"
                    android:ems="10"
                    android:imeOptions="actionNext"
                    android:inputType="text"
                    android:textColor="#69666E"
                    android:textSize="14sp" />

            </FrameLayout>

            <FrameLayout
                android:layout_width="150dp"
                android:layout_height="37dp"
                android:background="@drawable/join_birthday_1" >

                <EditText
                    android:id="@+id/yearView"
                    android:layout_width="104dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="right"
                    android:layout_marginRight="4dp"
                    android:background="@null"
                    android:ems="10"
                    android:imeOptions="actionNext"
                    android:inputType="number"
                    android:maxLength="4"
                    android:textColor="#69666E"
                    android:textSize="14sp" >

                    <requestFocus />
                </EditText>
            </FrameLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp" >

            <FrameLayout
                android:layout_width="150dp"
                android:layout_height="37dp"
                android:background="@drawable/join_id_1" >

                <EditText
                    android:id="@+id/signIdView"
                    android:layout_width="90.33dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="right"
                    android:layout_marginRight="4dp"
                    android:background="@null"
                    android:ems="10"
                    android:imeOptions="actionNext"
                    android:inputType="text"
                    android:textColor="#69666E"
                    android:textSize="14sp" />

            </FrameLayout>

            <Button
                android:id="@+id/btnCheckId"
                style="?android:attr/buttonStyleSmall"
                android:layout_width="75dp"
                android:layout_height="37dp"
                android:layout_marginLeft="20dp"
                android:background="@drawable/duplicatebtn_sel" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp" >

            <FrameLayout
                android:layout_width="320dp"
                android:layout_height="37dp"
                android:background="@drawable/join_pw_1" >

                <EditText
                    android:id="@+id/signPswdView"
                    android:layout_width="247.33dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="right"
                    android:layout_marginRight="4dp"
                    android:background="@null"
                    android:ems="10"
                    android:imeOptions="actionNext"
                    android:inputType="textPassword"
                    android:textColor="#69666E"
                    android:textSize="14sp" >

                </EditText>
            </FrameLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp" >

            <FrameLayout
                android:layout_width="320dp"
                android:layout_height="37dp"
                android:background="@drawable/join_pw_02_1" >

                <EditText
                    android:id="@+id/confirmPswdView"
                    android:layout_width="216dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="right|top"
                    android:layout_marginRight="4dp"
                    android:background="@null"
                    android:ems="10"
                    android:imeOptions="actionNext"
                    android:inputType="textPassword"
                    android:textColor="#69666E"
                    android:textSize="14sp" />

            </FrameLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="320dp"
            android:layout_height="37dp"
            android:layout_marginBottom="8dp"
            android:orientation="horizontal" >

            <FrameLayout
                android:layout_width="320dp"
                android:layout_height="37dp"
                android:background="@drawable/join_mail_1" >

                <LinearLayout
                    android:layout_width="258.66dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="right"
                    android:layout_marginRight="4dp" >

                    <EditText
                        android:id="@+id/signInEmailView"
                        android:layout_width="90dp"
                        android:layout_height="match_parent"
                        android:layout_gravity="right|center"
                        android:background="@null"
                        android:ems="10"
                        android:imeOptions="actionNext"
                        android:inputType="text"
                        android:textColor="#69666E"
                        android:textSize="14sp" />

                    <TextView
                        android:id="@+id/TextView03"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:text="@string/email_at" />

                    <EditText
                        android:id="@+id/signInEmailDomainView"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:background="@null"
                        android:ems="10"
                        android:imeOptions="actionDone"
                        android:inputType="text"
                        android:textColor="#69666E"
                        android:textSize="14sp" />
                </LinearLayout>
            </FrameLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="320dp"
            android:layout_height="37dp"
            android:layout_marginBottom="8dp" >

            <Spinner
                android:id="@+id/phoneNum1Spinner"
                android:layout_width="75dp"
                android:layout_height="37dp"
                android:layout_marginRight="6.66dp"
                android:background="@drawable/join_phone_01_1"
                android:textColor="#69666E"
                android:textSize="14sp" />

            <LinearLayout
                android:layout_width="75dp"
                android:layout_height="37dp"
                android:layout_marginRight="6.66dp"
                android:background="@drawable/join_phone_01_2"
                android:orientation="vertical" >

                <EditText
                    android:id="@+id/phoneNum2View"
                    android:layout_width="40dp"
                    android:layout_height="37dp"
                    android:layout_gravity="center"
                    android:background="@null"
                    android:ems="10"
                    android:imeOptions="actionNext"
                    android:inputType="phone"
                    android:maxLength="4"
                    android:textColor="#69666E"
                    android:textSize="14sp" >
                </EditText>
            </LinearLayout>

            <LinearLayout
                android:layout_width="75dp"
                android:layout_height="37dp"
                android:layout_marginRight="6.66dp"
                android:background="@drawable/join_phone_01_2"
                android:orientation="vertical" >

                <EditText
                    android:id="@+id/phoneNum3View"
                    android:layout_width="40dp"
                    android:layout_height="37dp"
                    android:layout_gravity="center"
                    android:background="@null"
                    android:ems="10"
                    android:imeOptions="actionDone"
                    android:inputType="phone"
                    android:maxLength="4"
                    android:textColor="#69666E"
                    android:textSize="14sp" />
            </LinearLayout>

            <!--
            <Button
                android:id="@+id/btnCheckPhone"
                style="?android:attr/buttonStyleSmall"
                android:layout_width="75dp"
                android:layout_height="37dp"
                android:background="@drawable/join_btn_02_1" />
            -->
        </LinearLayout>

        <LinearLayout
            android:layout_width="320dp"
            android:layout_height="37dp"
            android:layout_marginBottom="8dp"
            android:orientation="horizontal" >

            <CheckBox
                android:id="@+id/agreeCheck"
                android:layout_width="wrap_content"
                android:layout_height="21dp"
                android:layout_gravity="center"
                android:layout_marginRight="5dp"
                android:button="@drawable/checkbox_sel"
                android:checked="false" />

            <TextView
                android:id="@+id/textView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginRight="7dp"
                android:text="동의"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/stipulationView"
                android:layout_width="75dp"
                android:layout_height="match_parent"
                android:background="@drawable/stipulationbtn_sel"
                android:textAppearance="?android:attr/textAppearanceMedium" />

            <LinearLayout
                android:layout_width="74dp"
                android:layout_height="match_parent"
                android:layout_weight="0.23"
                android:gravity="right"
                android:orientation="vertical" >
            </LinearLayout>

            <RadioGroup
                android:id="@+id/genderGroup"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:orientation="horizontal" >

                <RadioButton
                    android:id="@+id/male"
                    android:layout_width="wrap_content"
                    android:layout_height="21dp"
                    android:button="@drawable/radiobtn_sel"
                    android:checked="true"
                    android:text=" 남  " />

                <RadioButton
                    android:id="@+id/female"
                    android:layout_width="wrap_content"
                    android:layout_height="21dp"
                    android:button="@drawable/radiobtn_sel"
                    android:text=" 여" />
            </RadioGroup>
        </LinearLayout>
    </LinearLayout>

    <Button
        android:id="@+id/btnRegister"
        android:layout_width="170dp"
        android:layout_height="32dp"
        android:layout_marginBottom="23dp"
        android:layout_marginLeft="95dp"
        android:layout_marginRight="95dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/finishbtn_sel" />

</LinearLayout>